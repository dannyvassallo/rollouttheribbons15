  <script type="text/javascript">window.tab_properties  = $.extend(window.tab_properties, {"tab_meta":{"title":"Dedicate a Ribbon","picture":"https:\/\/www.myfangate.com\/ci\/uploads\/tabimages\/ROTR_tab1.png","description":"To raise cancer awareness, Roll Out The Ribbons is giving people an opportunity to dedicate and post ribbons. Dedicate your ribbon today! http:\/\/fngt.it\/Rollouttheribbons?utm_source=facebook&utm_medium=original&utm_content=post&utm_campaign=ROR"}});
